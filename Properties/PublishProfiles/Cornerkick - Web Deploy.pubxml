<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/7cf03132-31ae-48b4-b930-d8a008f02ad9/resourcegroups/ckRessourceGroup/providers/Microsoft.Web/sites/Cornerkick</ResourceId>
